<p id="notice"><%= notice %></p>

<h1>Listing Results</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @results.each do |result| %>
      <tr>
        <td><%= result %></td>
        <td><%= link_to 'Show', result %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
